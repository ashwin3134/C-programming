#include<stdio.h>

void swap(int*num1,int*num2){
    int temp;
    temp=*num1;
    *num1=*num2;
    *num2=temp;
}
int main(){
    int num1,num2;
    printf("Enter the 1st number: ");
    scanf("%d",&num1);
    printf("Enter the 2nd number: ");
    scanf("%d",&num2);
    
    swap(&num1,&num2);
    
    printf("The 1st number: %d\n",num1);
    printf("The 2nd number: %d\n",num2);
    
    return (0);
}
